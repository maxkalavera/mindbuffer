{
  "name": "mindbuffer",
  "version": "0.5.7",
  "description": "A note's manager app with the visuals of a Webchat",
  "repository": "https://github.com/maxkalavera/mindbuffer",
  "author": "Max Hernandez",
  "license": "MIT",
  "main": ".webpack/main/main.js",
  "scripts": {
    "webpack:build:main": "webpack --config webpack.main.config.js",
    "webpack:build:renderer": "webpack --config webpack.renderer.config.js",
    "webpack:build:preload": "webpack --config webpack.preload.config.js",
    "webpack:build": "npm run webpack:build:main && npm run webpack:build:renderer && npm run webpack:build:preload",
    "electron:start": "electron .",
    "start": "cross-env MINDBUFFER_ENVIRONMENT=development npm run webpack:build && concurrently --kill-others --kill-others-on-fail -c \"yellow,gray\" \"webpack --config webpack.renderer.config.js --watch\" \"electron .\""
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.3",
    "html-webpack-plugin": "^5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
