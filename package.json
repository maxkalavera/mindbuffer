  },
  "description": "A note's manager app with the visuals of a Webchat",
  "productName": "MindBuffer",
  "version": "0.5.7",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env MINDBUFFER_ENVIRONMENT=development MINDBUFFER_DEBUG=true electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "cross-env MINDBUFFER_ENVIRONMENT=testing electron-forge package && concurrently --kill-others --kill-others-on-fail -c \"gray,yellow\" \"chromedriver\" \"jest --runInBand\"",
    "test-package": "cross-env MINDBUFFER_ENVIRONMENT=testing electron-forge package",
    "test-execute": "concurrently --kill-others --kill-others-on-fail -s \"last\" -c \"gray,yellow\" \"chromedriver\" \"jest --runInBand $TEST_FILE\""
  },
